input_parse = []
for row in inputs:
    if row == '':
        input_parse.append([])
    else:
        input_parse[-1].append(row)

'''
notes=0
for pic in input_parse:
    prior_row=''
    height = len(pic)
    for r,row in enumerate(pic):
        mirror_found = False
        if row == prior_row:
            short_height = min(height - r, r)
            mirror_found=True
            for i in range(short_height):
                norm_r = r+i
                mirror_r = r-i-1
                if pic[norm_r] != pic[mirror_r]:
                    mirror_found=False
                    break
        if mirror_found:
            notes += 100 * r
            break
        prior_row = row

    prior_col = [] 
    cur_col = []
    for c in range(len(pic[0])):
        width = len(pic[0])
        cur_col = [row[c] for row in pic]
        mirror_found = False
        if cur_col == prior_col:
            short_width = min(width - c, c)
            mirror_found = True
            for i in range(short_width):
                norm_c = c+i
                mirror_c = c-i-1
                norm_col   = [row[norm_c] for row in pic]
                mirror_col = [row[mirror_c] for row in pic]
                if norm_col != mirror_col:
                    mirror_found = False
                    break
        if mirror_found:
            notes += c
            break
        prior_col = cur_col

print('p1:', notes)

'''
# P2
'''
input_parse = [[
'#.##..##.',
'..#.##.#.',
'##......#',
'##......#',
'..#.##.#.',
'..##..##.',
'#.#.##.#.',
],[
'#...##..#',
'#....#..#',
'..##..###',
'#####.##.',
'#####.##.',
'..##..###',
'#....#..#',
]]
'''

def print_pic(pic):
    for row in pic:
        print(row)
    print()

#for pic in input_parse:
#    print('orig pic')
#    print_pic(pic)

def find_mirror(pic, skip_r=0, skip_c=0):
    #print_pic(pic)
    notes=0
    prior_row=''
    height = len(pic)
    for r,row in enumerate(pic):
        if r == skip_r:
            prior_row = row
            continue
        mirror_found = False
        if row == prior_row:
            short_height = min(height - r, r)
            mirror_found=True
            for i in range(short_height):
                norm_r = r+i
                mirror_r = r-i-1
                if pic[norm_r] != pic[mirror_r]:
                    mirror_found=False
                    break
        if mirror_found:
            notes += 100 * r
            break
        prior_row = row

    mirror_found_r = mirror_found
    if not mirror_found:
        prior_col = [] 
        cur_col = []
        for c in range(len(pic[0])):
            cur_col = [row[c] for row in pic]
            if c == skip_c:
                prior_col = cur_col
                continue
            width = len(pic[0])
            mirror_found = False
            if cur_col == prior_col:
                short_width = min(width - c, c)
                mirror_found = True
                for i in range(short_width):
                    norm_c = c+i
                    mirror_c = c-i-1
                    norm_col   = [row[norm_c] for row in pic]
                    mirror_col = [row[mirror_c] for row in pic]
                    if norm_col != mirror_col:
                        mirror_found = False
                        break
            if mirror_found:
                notes += c
                break
            prior_col = cur_col
        if mirror_found_r == True and mirror_found:
            print('Row and COLUMN FOUND!')
    return(notes)

total_notes=0
for pic in input_parse:
    old_notes = find_mirror(pic)
    skip_r = int(old_notes/100)
    skip_c = old_notes % 100
    
    notes_list=[0]
    for r, row in enumerate(pic):
        for c,item in enumerate(row):
            found_mirror=False
            if item =='.':
                out = '#'
            elif item =='#':
                out = '.'
            else:
                print('ERROR!')
            cur_row = row[0:c] + out + row[c+1:]
            #print('cur_row', cur_row)
            cur_pic = [row for row in pic]
            #print('cur_pic')
            #print_pic(cur_pic)
            cur_pic[r]=cur_row
            #print('cur_pic_after_mod')
            #print_pic(cur_pic)

            notes = find_mirror(cur_pic,skip_r, skip_c)
            #print(notes)
            if notes > 0 and notes != old_notes:
                notes_list.append(notes)
                found_mirror=True
                print('notes',notes)
                print('r,c',r,c)
                #print_pic(cur_pic)
                break

    if len(notes_list)==1:
        print("Nothing_found")
        print_pic(pic)
        print("orig score", find_mirror(pic))
    total_notes+=max(notes_list)
    print(total_notes)
        #if found_mirror==True:
        #    break

# 24950 was too low
# 36049 was too low
# 37574 was wrong

notes=find_mirror(input_parse[0])
print('notes',notes)

print('p2:', total_notes)





            

