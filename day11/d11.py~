#'''
inputs=[
'............',
'....#.......',
'........#...',
'.#..........',
'............',
'.......#....',
'..#.........',
'..........#.',
'............',
'........#...',
'.#...#......',
'............',
]
row_pad= '............'
#'''
from copy import deepcopy
inputp1 = deepcopy(inputs)

for r in range(len(inputp1)-1,-1,-1):
    if set(inputp1[r]) == set('.'):
        inputp1.insert(r,row_pad)

for c in range(len(inputp1[0])-1,-1,-1):
    found_galaxy = False
    for r in range(len(inputp1)):
        if inputp1[r][c] == "#":
            found_galaxy=True
            break
    if not found_galaxy:
        for r in range(len(inputp1)):
            inputp1[r]= inputp1[r][0:c] + "." + inputp1[r][c:]

galaxies = []
for r, row in enumerate(inputp1):
    for c,item in enumerate(row):
        if item == "#":
            galaxies.append([r,c])

galaxy_dists = []

for galaxy1 in galaxies:
    for galaxy2 in galaxies:
        y_diff = abs(galaxy2[0]-galaxy1[0])
        x_diff = abs(galaxy2[1]-galaxy1[1])
        galaxy_dists.append(x_diff+y_diff)

#for row in inputp1:
#    print(row)
print(galaxy_dists)
print()
print('p1', int(sum(galaxy_dists)/2)) # distance code above is naive and doing double the work: distances there and reverse so divide by 2.

# P2
from copy import deepcopy
inputp2 = deepcopy(inputs)
row_expansions=[]
for r in range(len(inputp2)-1,-1,-1):
    if set(inputp2[r]) == set('.'):
        row_expansions.append(r)

col_expansions=[]
for c in range(len(inputp2[0])-1,-1,-1):
    found_galaxy = False
    for r in range(len(inputp2)):
        if inputp2[r][c] == "#":
            found_galaxy=True
            break
    if not found_galaxy:
        col_expansions.append(c)

galaxies = []
for r, row in enumerate(inputp2):
    for c,item in enumerate(row):
        if item == "#":
            galaxies.append([r,c])

galaxy_dists = []

for galaxy1 in galaxies:
    for galaxy2 in galaxies:
        min_row_galaxy = min(galaxy2[0],galaxy1[0])
        max_row_galaxy = max(galaxy2[0],galaxy1[0])
        expansions = 0
        for row_expand in row_expansions:
            if row_expand > min_row_galaxy and row_expand < max_row_galaxy:
                expansions+=1
        min_col_galaxy = min(galaxy2[1],galaxy1[1])
        max_col_galaxy = max(galaxy2[1],galaxy1[1])
        for col_expand in col_expansions:
            if col_expand > min_col_galaxy and col_expand < max_col_galaxy:
                expansions+=1

        y_diff = abs(galaxy2[0]-galaxy1[0])
        x_diff = abs(galaxy2[1]-galaxy1[1])
        galaxy_dists.append(x_diff+y_diff+999999*expansions) # instructions said to *replace* empty rows/cols with 1,000,000, but I never removed the original.

#for row in inputp2:
#    print(row)
print()
print('p2', int(sum(galaxy_dists)/2)) # distance code above is naive and doing double the work: distances there and reverse so divide by 2.




        


    
     



